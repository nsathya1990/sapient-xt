{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
        "name": "rickandmortyapi.com",
        "urls": ["https://rickandmortyapi.com/api/character/avatar/**"],
        "cacheConfig": {
            "maxSize": 3,
            "maxAge": "15m",
            "strategy": "performance"
        }
    }
]
}
